<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<simulation>

  <!-- General - geometry definitions !-->
  <geometry>
    <patchfile type="bsplines">Lshape.g2</patchfile>
   <raiseorder patch="1" u="1" v="1"/>
    <refine type="uniform" patch="1" u="7" v="7"/>
    <topologysets>
      <set name="Homogenous" type="edge">
        <item patch="1">1 2 3 4</item>
      </set>
    </topologysets>
  </geometry>

  <!-- General - boundary conditions !-->
  <boundaryconditions>
    <dirichlet set="Homogenous" comp="1"/>
 </boundaryconditions>

  <!-- Problem-specific block !-->
  <advectiondiffusion>
    <source type="expression">100*sqrt((x-0.5)*(x-0.5)+(y-0.5)*(y-0.5))*(sqrt((x-0.5)*(x-0.5)+(y-0.5)*(y-0.5))-0.5)*(sqrt((x-0.5)*(x-0.5)+(y-0.5)*(y-0.5))-1/sqrt(2))</source>
    <advectionfield>3|1</advectionfield>
    <reactionfield>1</reactionfield>
    <stabilization type="MS" Cinv="1.0"/>
    <fluidproperties kappa="1e-6" Pr="0.5"/>
   <kappa>1e-6</kappa>
   <prandtl>0.5</prandtl>
  </advectiondiffusion>
</simulation>
